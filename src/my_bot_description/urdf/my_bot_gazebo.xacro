<?xml version="1.0"?>




<robot  name="my_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <gazebo reference="wheel_right_link">
        <mu1>100000000000000.0</mu1>
        <mu2>100000000000000.0</mu2>
        <kp>1000000000000.0</kp>
        <kd>10.0</kd>
        <mindepth>0.001</mindepth>
        <maxvel>0.1</maxvel>
        <fdir1>1 0 0</fdir1>

    </gazebo>
    <gazebo reference="wheel_left_link">
        <mu1>100000000000000.0</mu1>
        <mu2>100000000000000.0</mu2>
        <kp>1000000000000.0</kp>
        <kd>10.0</kd>
        <mindepth>0.001</mindepth>
        <maxvel>0.1</maxvel>
        <fdir1>1 0 0</fdir1>

    </gazebo>
    <gazebo reference="caster_front_link">
         <mu1>0.1</mu1>
        <mu2>0.1</mu2>
        <kp>1000000.0</kp>
        <kd>100.0</kd>
        <mindepth>0.001</mindepth>
        <maxvel>0.1</maxvel>

    </gazebo>
    <gazebo reference="caster_rear_link">
        <mu1>0.1</mu1>
        <mu2>0.1</mu2>
        <kp>1000000.0</kp>
        <kd>100.0</kd>
        <mindepth>0.001</mindepth>
        <maxvel>0.1</maxvel>
        

    </gazebo>
    <gazebo> 
        <xacro:if value="$(arg is_ignition)">
            <plugin filename="ign_ros2_control-system" name="ign_ros2_control::IgnitionROS2ControlPlugin">
                <parameters/>
            </plugin>
        </xacro:if>
        <xacro:unless value="$(arg is_ignition)">
            <plugin filename="gz_ros2_control-system" name="gz_ros2_control::GazeboROS2ControlPlugin">
                <parameters/>
            </plugin>
        </xacro:unless>

    </gazebo>
</robot>